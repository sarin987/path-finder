{
  "name": "safety-emergency-backend",
  "version": "1.0.0",
  "description": "Backend for Safety Emergency Application",
  "main": "server.mjs",
  "scripts": {
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node server.mjs",
    "dev": "NODE_ENV=development nodemon --experimental-specifier-resolution=node server.mjs",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "sync-db": "node scripts/sync-db.js",
    "test:auth": "node scripts/test-auth.js",
    "test": "NODE_ENV=test jest --detectOpenHandles --forceExit",
    "test:watch": "NODE_ENV=test jest --watch --detectOpenHandles --forceExit",
    "test:coverage": "NODE_ENV=test jest --coverage --detectOpenHandles --forceExit"
  },
  "type": "module",
  "imports": {
    "#src/*": "./src/*.js"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "dependencies": {
    "@types/multer": "^1.4.13",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^24.2.1",
    "@types/socket.io": "^3.0.1",
    "@types/socket.io-client": "^1.4.36",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^30.0.0",
    "jest": "^30.0.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.1",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
